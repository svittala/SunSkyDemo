            </connector>
            <label>Create Schedule Based Jobs</label>
        </rules>
    </decisions>
    <decisions>
        <name>If_below_the_occurance_count</name>
        <label>If below then occurance count</label>
        <locationX>448</locationX>
        <locationY>731</locationY>
        <defaultConnector>
            <targetReference>Create_a_Delete_Record_for_Schedule</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>[Default Outcome]</defaultConnectorLabel>
        <rules>
            <name>within_occurance_schedule</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varCount</leftValueReference>
                <operator>LessThan</operator>
                <rightValue>
                    <elementReference>varOpptyLineRec.occurrence_count__c</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>createJobRecord</targetReference>
            </connector>
            <label>within occurance schedule</label>
        </rules>
    </decisions>
