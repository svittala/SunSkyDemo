    <decisions>
        <name>checkifScheduleType</name>
        <label>checkifScheduleType</label>
        <locationX>458</locationX>
        <locationY>384</locationY>
        <defaultConnector>
            <targetReference>create_a_non_schedule_job_record</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Not Schedule based</defaultConnectorLabel>
        <rules>
            <name>Create_Schedule_Based_Jobs</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varOpptyLineRec.HasSchedule</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
