    <fields>
        <fullName>Account__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <description>Relation ship to Account -</description>
        <externalId>false</externalId>
        <label>Account</label>
        <referenceTo>Account</referenceTo>
        <relationshipLabel>Jobs</relationshipLabel>
        <relationshipName>Jobs</relationshipName>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Assignment_Count__c</fullName>
        <description>count of the employees assigned to this job</description>
        <externalId>false</externalId>
        <label>Assignment Count</label>
        <summaryForeignKey>Job_Assignment__c.Job__c</summaryForeignKey>
        <summaryOperation>count</summaryOperation>
        <trackTrending>false</trackTrending>
        <type>Summary</type>
    </fields>
    <fields>
        <fullName>Commission_Amount__c</fullName>
        <description>Commission Amount Per Employee Assigned for the Job</description>
        <externalId>false</externalId>
        <formula>Unit_Price__c / ( 2 )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Commission Amount to the Employee at 50% rate</inlineHelpText>
        <label>Commission Amount</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Commission_Paid__c</fullName>
        <defaultValue>false</defaultValue>
        <description>Commission Paid to the Employee</description>
        <externalId>false</externalId>
        <inlineHelpText>If the Commission is Paid to Employee</inlineHelpText>
        <label>Commission Paid</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>JobProduct__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <externalId>false</externalId>
        <label>JobProduct</label>
        <referenceTo>Product2</referenceTo>
        <relationshipLabel>Jobs</relationshipLabel>
        <relationshipName>Jobs</relationshipName>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
