        <type>AutoNumber</type>
    </nameField>
    <pluralLabel>Jobs</pluralLabel>
    <searchLayouts/>
    <sharingModel>ReadWrite</sharingModel>
    <validationRules>
        <fullName>All_Assignment_Completed</fullName>
        <active>true</active>
        <description>Confirm the Assignment is Complete</description>
        <errorConditionFormula>AND(  Job_Assigned__c ,(Quantity__c  &lt;&gt;   Assignment_Count__c))</errorConditionFormula>
        <errorMessage>Please make sure to assign employees before marking the Job as assigned.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Job_Completed</fullName>
        <active>true</active>
        <description>Confirm the Assignment is Complete before Job can be marked as complete</description>
        <errorConditionFormula>AND(   Job_Completed__c , Not(Job_Assigned__c ))</errorConditionFormula>
        <errorMessage>Please make sure to assign employees before closing the Job.</errorMessage>
    </validationRules>
</CustomObject>
